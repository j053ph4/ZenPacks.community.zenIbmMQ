<object id='/zport/dmd/Events/App/MQ' module='Products.ZenEvents.EventClass' class='EventClass'>
<property type="text" id="transform" mode="w" >
setPDKey('ibmMqServiceKey')


#find the event component
evcomponent = findEventComponent(device, evt.component)

# if it's a MQManager and the status is "shutdown" then unmonitor
if evcomponent is not None:
	if evcomponent.meta_type == 'MQManager':	
		try:
			val = int(getMetricValue(evt.summary))
		except: val = -1
		evt.val = val
		evt.valkeys = ' '.join(str(x) for x in evcomponent.managerRunStateMap.keys())
		try:  mgrstatus = evcomponent.managerRunStateMap[val]
		except: mgrstatus = 'Unknown'
		evt.mgrstatus = mgrstatus
		try:
			msg = "MQ Manager %s is %s" % (evcomponent.managerName, mgrstatus)
			evt.summary = msg
			# disable monitoring if it is shut down
			if mgrstatus == 'Shutdown':
				evt.severity = 2
				#monitorToggle(evcomponent, False)
				#txnCommit()
		except: pass
